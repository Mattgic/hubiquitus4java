<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean id="MyFirstBotInterface" class="org.hubiquitus.hubots.myfirstbot.service.impl.MyFirstBotInterfaceImpl">
		<property name="hubotConfigMap" ref="hubotConfigMap" />
	</bean>

		
	<bean id="MyItemEventListener" class="org.hubiquitus.hubots.myfirstbot.application.query.MyItemEventListener">
	  	<property name="ProcessMessage" ref="ProcessMessage"/>
	</bean>

	
	<bean id="hubot" class="org.hubiquitus.hubots.myfirstbot.application.MyFirstBot">
	  	<property name="exceptionCollector" ref="exceptionCollector" />
	  	<property name="xmppEntryPublisher" ref="xmppEntryPublisher"/>
	  	<property name="hubotConfig" ref="MyFirstBotInterface"/>
	  	<property name="jSonParser" ref="jSonParser"/>
	  	<property name="myItemEventListener" ref="MyItemEventListener"/>
	</bean>

</beans>